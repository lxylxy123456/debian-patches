-
-                desc = description
-                desc.set_size(newsize * Pango.SCALE)
-
-                newtag.props.font_desc = desc
-                self._font_tags[newsize] = newtag
-            else:
-                newtag = self._font_tags[newsize]
-
-            # Remove all the previous mix of tags
